{
	"name": "vl-ui",
	"version": "0.2.0",
	"private": true,
	"dependencies": {
		"@apollo/client": "^3.3.15",
		"@material-ui/core": "^4.11.3",
		"@material-ui/icons": "^4.11.2",
		"@mdi/font": "^5.9.55",
		"@mdi/js": "^5.9.55",
		"@mdi/react": "^1.5.0",
		"@reduxjs/toolkit": "^1.5.1",
		"@stripe/react-stripe-js": "^1.3.0",
		"@stripe/stripe-js": "^1.13.2",
		"@types/jest": "^26.0.22",
		"@types/node": "^14.14.41",
		"@types/react": "^17.0.3",
		"@types/react-dom": "^17.0.3",
		"@types/react-redux": "^7.1.9",
		"axios": "^0.21.0",
		"chokidar-cli": "^2.1.0",
		"classnames": "^2.3.1",
		"file-saver": "^2.0.5",
		"graphql": "^15.5.0",
		"lodash": "^4.17.20",
		"nanoid": "^3.1.20",
		"npm-run-all": "^4.1.5",
		"postcss-extend": "^1.0.5",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-editor-js": "^1.6.2",
		"react-ga": "^3.3.0",
		"react-gtm-module": "^2.0.11",
		"react-helmet-async": "^1.0.7",
		"react-hook-form": "^6.15.4",
		"react-input-autosize": "^3.0.0",
		"react-markdown": "^5.0.3",
		"react-redux": "^7.2.3",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^4.0.3",
		"react-window": "^1.8.5",
		"tailwindcss": "^1.9.6",
		"typescript": "^4.2.4"
	},
	"scripts": {
		"build:tailwind": "postcss src/client/styles/main.scss -o src/client/styles/main.output.css",
		"watch:tailwind": "chokidar 'client/src/**/*.css' 'src/**/*.scss' --ignore src/client/styles/main.output.css -c 'npm run build:tailwind'",
		"start:react": "react-scripts start",
		"start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
		"prebuild": "run-s build:tailwind",
		"build": "react-scripts build",
		"lint": "eslint ./src --ext .js,.ts,.jsx,.tsx",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"deploy:dev": "REACT_APP_STAGE=dev && yarn build && amplify env checkout dev && amplify publish",
		"deploy:prod": "REACT_APP_STAGE=prod && yarn build && amplify env checkout prod && amplify publish"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.12.0",
		"@testing-library/react": "^11.2.6",
		"@testing-library/user-event": "^13.1.5",
		"@types/classnames": "^2.3.1",
		"@types/react-transition-group": "^4.4.0",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"autoprefixer": "^9.8.6",
		"axios-mock-adapter": "^1.19.0",
		"eslint": "^7.24.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-config-prettier": "^8.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest-dom": "^3.8.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-testing-library": "^4.1.1",
		"history": "^4.10.1",
		"husky": "^6.0.0",
		"install": "^0.13.0",
		"jest-junit": "^12.0.0",
		"jest-localstorage-mock": "^2.4.10",
		"lint-staged": "^10.5.4",
		"npm": "^7.11.1",
		"postcss": "^8.1.8",
		"postcss-cli": "^7.1.1",
		"postcss-comment": "^2.0.0",
		"postcss-import": "^12.0.1",
		"postcss-nested": "^4.2.3",
		"prettier": "^2.1.2",
		"redux-mock-store": "^1.5.4"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --write"
		],
		"*.{js,ts,jsx,tsx}": "eslint --cache --fix"
	}
}
